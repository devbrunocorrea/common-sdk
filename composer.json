{
    "name": "gpupo/common-sdk",
    "description": "Componente de uso comum entre SDKs para integração a partir de aplicações PHP com as APIs de Marketplaces",
    "keywords": ["sdk"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\Tests\\CommonSdk\\": "tests"
        }
    },
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "gpupo/cache": "~1.2",
        "gpupo/common": "~1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "minimum-stability" : "stable"
}
