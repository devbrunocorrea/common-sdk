{
    "name": "gpupo/common-sdk",
    "description": "Componente de uso comum entre SDKs para integração a partir de aplicações PHP com Restful webservices",
    "homepage": "http://www.g1mr.com/common-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "g@g1mr.com"
        }
    ],
    "keywords": ["sdk"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonSdk\\": "src",
            "Gpupo\\Tests\\CommonSdk\\": "tests"
        }
    },
    "repositories": [
       {
         "type": "vcs",
         "url": "git@github.com:gpupo/peek-and-poke.git",
         "reference": "php54Support"
       }
   ],
    "require": {
        "php": ">=5.4",
        "psr/log": "~1.0",
        "gpupo/cache": "~1.2",
        "gpupo/common": "~1.5.1",
        "twig/twig" : "~1.20.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeclimate/php-test-reporter": "dev-master",
        "sebastian/peek-and-poke": "1.0.x@dev"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability" : "stable"
}
