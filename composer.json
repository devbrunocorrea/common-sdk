{
    "name": "gpupo/common-sdk",
    "description": "Common SDK Objects",
    "keywords": ["sdk"],
    "license": "MIT",
    "autoload": {
        "psr-0": { "Gpupo": "src" }
    },
    "require": {
        "php": ">=5.5",
        "doctrine/common": "*",
        "psr/log": "*"
    },
    "extra": {
